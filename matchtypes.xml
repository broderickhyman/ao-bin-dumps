  <!-- MATCHES -->

  <!-- ============================== DEBUG ============================== -->
  <matchtype uniquename="MATCH_TEST" 
             matchdurationminutes="25" 
             roundduration="60" 
             ticketdefault="150" 
             timeleftorbearlyreset="60" 
             respawndelay="10" 
             matchtimerespawnmultiplier="1" 
             minplayerperteam="1"
             maxplayerperteam="5" 
             ticketreductionbydeath="4" 
             almostlostticketthreshold="10" 
             advantageticketsdifference="50">
    
    <ticketreductionlookup orbdifference="0" ticketreduction="0" />
    <ticketreductionlookup orbdifference="1" ticketreduction="10" />
    <ticketreductionlookup orbdifference="2" ticketreduction="10" />
    <ticketreductionlookup orbdifference="3" ticketreduction="20" />
    <ticketreductionlookup orbdifference="4" ticketreduction="20" />
    <warcamp>
      <defense engageattackrange="18.0" 
               disengageattackrange="18.0" 
               attackinterval="0.4" 
               attackdamage="0.1" 
               attackdistancedelay="24.0" 
               islethal="true" 
               buffradius="8.7" 
               buffspell="WARCAMP_INVULNERABILITY">
        <FriendlyFX beam="FX/ClientPrefabs/fx_beam_gvg_blue" impact="Prefabs/SpellEffects/MeleeHit"/>
        <HostileFX beam="FX/ClientPrefabs/fx_beam_gvg_orange" impact="Prefabs/SpellEffects/MeleeHit"/>
      </defense>
    </warcamp>
  </matchtype>
 
